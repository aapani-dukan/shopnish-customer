{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "nodeMethod": "npm",
      "installCommand": "npm install --legacy-peer-deps"
    },
    "production": {
      "autoIncrement": true,
      "nodeMethod": "npm",
      "installCommand": "npm install --legacy-peer-deps"
    }
  },
  "submit": {
    "production": {}
  }
}